<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dictionary language="mysql">
    <translation language="mysql" element="Template">
        <elementTranslation>
            create or replace trigger [TriggerName]
                before [TimeOperator]
                on [TableName]
                for each row
            declare
                [Variables]
            begin
                if not [Conditions] then
                    [Error]
                end if;
            end [TriggerName];
        </elementTranslation>
    </translation>
